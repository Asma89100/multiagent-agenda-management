body
  margin 0px
  padding 20px
  font: 14px "Lucida Grande", Helvetica, Arial, sans-serif
a
  color: #00B7FF

.hidden
  display none
  visibility hidden

#calendar td
  padding 8px

#calendar thead
  td:FIRST-CHILD
    width 80px
    color #000000
    background-color #ffffff
  td
    text-align center
    width 100px
    background-color #585858
    color #ffffff
    
#calendar tbody
  td:FIRST-CHILD
    text-align center
    color #000000
    background-color #c0c0c0
  td
    background-color #e6e6e6